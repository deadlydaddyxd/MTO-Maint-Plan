{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/next",
      "config": {
        "distDir": ".next",
        "outputFileTracingRoot": "../"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://veh-maint-app-production.up.railway.app/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://veh-maint-app-production.up.railway.app/api"
  }
}