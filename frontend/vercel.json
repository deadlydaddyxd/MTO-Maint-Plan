{
  "version": 2,
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "outputDirectory": ".next",
  "env": {
    "NODE_ENV": "production",
    "NEXT_PUBLIC_API_URL": "https://veh-maint-app-production.up.railway.app/api"
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://veh-maint-app-production.up.railway.app/api/:path*"
    }
  ]
}